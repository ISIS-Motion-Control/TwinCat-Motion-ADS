<Window x:Class="TwinCat_Motion_ADS.AddDataColumnWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TwinCat_Motion_ADS"
        mc:Ignorable="d"
        Height="250" Width="400" ResizeMode="NoResize" Topmost="True">
    <StackPanel>
        <TextBox x:Name="ColumnTitle" Height="20" Width="150" VerticalAlignment="Top"/>
        <ComboBox x:Name="Combo_Header1" Width="200" Height="25"/>
        <ComboBox x:Name="Combo_Header2" Width="200" Height="25"/>
        <Button x:Name="Button_Add" Content="Add" Width="100" Height="25" Click="Button_Add_Click"/>
        <Button x:Name="Button_Cancel" Content="Cancel" Width="100" Height="25" Click="Button_Cancel_Click"/>
    </StackPanel>
</Window>
